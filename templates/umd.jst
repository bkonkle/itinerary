    // only CommonJS-like environments that support module.exports,
    // like Node.
    module.exports = factory(<%= commonDeps.join(', ') %>);
  } else {
    // Browser globals (root is window)
    root.<%= namespace %> = factory(<%= globalDeps.join(', ') %>);
  }
}(this, function (<%= _.values(deps).join(', ') %>) {

